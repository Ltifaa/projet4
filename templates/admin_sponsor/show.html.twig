{% extends 'basebackend.html.twig' %}

{% block title %}Sponsor{% endblock %}

{% block body %}
 <a href="{{ path('app_admin_sponsor_index') }}" class="btn btn-dark mb-3">Retour</a>
    <h1>Sponsor</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sponsor.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ sponsor.name }}</td>
            </tr>
            <tr>
                <th>ImageName</th>
                <td>{{ sponsor.imageName }}</td>
            </tr>
            <tr>
                <th>SiteWeb</th>
                <td>{{ sponsor.siteWeb }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ sponsor.slug }}</td>
            </tr>
        </tbody>
    </table>

   

    <a href="{{ path('app_admin_sponsor_edit', {'id': sponsor.id}) }}" class="btn btn-success mb-3">Modifier</a>

    {{ include('admin_sponsor/_delete_form.html.twig') }}
{% endblock %}
